#include <stdio.h>
#include "Global.h"
#include "Find.h"

void test_build_data_file_index() {
  FILE * f = fopen(DATA_FILE, "rb");
  file_position **file_index;
  int count;
  
  count = build_data_file_index(&file_index, f);

  int i;
  for(i = 0; i < count; i++) {
    printf("%d %ld %d\n", file_index[i]->id,
	   file_index[i]->position,
	   file_index[i]->size);
  }
}

int main() {
  test_build_data_file_index();
  return 0;
}
