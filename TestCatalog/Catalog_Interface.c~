#include "Catalog.h"
#include <stdio.h>
#include <malloc.h>
#include <string.h>
#include "Global.h"

bool catalog_update(char * Key_name, char * Key_Type) {
  if(Key_name == NULL || Key_Type == NULL) {
    return false;
  }

  Catalog_append(&CATALOG, Key_name, Key_Type);

  if(CATALOG == NULL) {
    return false;
  }

  return true;
}

bool catalog_build() {
  FILE * F_Catalog = fopen("CATALOG_FILE", "r");
  if(F_Catalog == NULL) {
    return false;
  }
  
}

bool catalog_save() {

}
